 <header class="header_section" style="background-color: black;">
      <div class="container">
        <nav class="navbar navbar-expand-lg custom_nav-container ">
          <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">
            <span>
              FPT BOOK
            </span>
          </a>

          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class=""> </span>
          </button>

          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav  mx-auto ">
              <li class="nav-item active">
                <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">Home <span class="sr-only">(current)</span></a>
              </li>
              <li class="nav-item">
                <a class="nav-link" asp-area="Identity" asp-page="/Account/Manage/index">Profile</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" asp-area="" asp-controller="Cart" asp-action="index">Cart</a>
              </li>
              @if (User.IsInRole("Admin")){
              <li class="nav-item">
                <a class="nav-link" asp-area="" asp-controller="Product" asp-action="index">Management</a>
              </li>
              }
               @if (User.IsInRole("Store Owner")){
              <li class="nav-item">
                <a class="nav-link" asp-area="" asp-controller="Product" asp-action="index">Management</a>
              </li>
              }

              <li class="nav-item">
                <a class="nav-link" asp-controller="Home" asp-action="Help">HELP</a>
              </li>
              @if(!User.Identity.IsAuthenticated){
              <li class="nav-item">
                <a class="nav-link" asp-area="Identity" asp-page="/Account/Login">Log in</a>
              </li>
              }else{
              <li class="nav-item">
                <a class="nav-link" asp-area="Identity" asp-page="/Account/Logout">Logout</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" asp-area="Identity" asp-page="/Account/ResendEmailConfirmation">Reset Password</a>
              </li>
              }
              <li>
                <form asp-controller="Home" asp-action="search" method="get">
                  <div class="input-group">
                    <input type="text" class="form-control" name="search" placeholder="Search term...">
                    <span class="input-group-btn">
                      <button type="submit" class="btn btn-primary">Search</button>
                    </span>
                  </div>
                </form>
              </li>
            <li>
            </ul>
          </div>
        </nav>
      </div>
    </header>
    <!-- end header section -->
